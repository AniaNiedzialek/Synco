{
    "manifest_version":3,
    "name": "Synco",
    "version": "1.0",
    "description": "A collaborative tool to help students manage group projects",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Synco"
    },
    "icons": {
        "128": "images/logo.png"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://127.0.0.1:8000/",
        "http://127.0.0.1:8000/api/tasks/",
        "http://127.0.0.1:8000/api/api-token-auth/"
]

}